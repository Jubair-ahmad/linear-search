#include <stdio.h>

int main()
{
  int  A[10]={2,4,5,6,7,8,1,9,34,12};
  int i,search,pos=0;
  printf("enter a value to be search\n");
  scanf("%d",&search);
  for(i=0;i<10;i++){
      if(A[i]==search){
     pos=i+1;
  }
  }
  if(pos !=0){
    printf("%d is present at %d position",search,pos);}
        else{
            printf("not present");
        }
    return 0;
  }
